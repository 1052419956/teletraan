<ul class="nav nav-tabs">
    {% for stage in stages %}
    <li {% if stage == env.stageName %}class="active"{% endif %}>
    <a class="deployToolTip" data-container="body" data-toggle="tooltip" title="{{env.description}}" data-placement="bottom"
       href="/env/{{ env.envName }}/{{ stage }}/{{ envTabKind }}">{{ stage }}</a>
    </li>
    {% endfor %}
</ul>
